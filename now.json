{
  "version": 2,
  "builds": [
    {
      "src": "/examples/breakout/package.json",
      "use": "@now/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "^/examples/breakout/(.*).js$",
      "dest": "/examples/breakout/$1.js"
    },
    {
      "src": "^/examples/breakout/(.*).png$",
      "dest": "/examples/breakout/$1.png"
    },
    {
      "src": "^/examples/breakout/(.*).json$",
      "dest": "/examples/breakout/$1.json"
    },
    {
      "src": "^/examples/breakout/(.*)",
      "dest": "/examples/breakout/index.html"
    }
  ]
}
